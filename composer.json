{
	"name": "lns/digital-asset-management",
	"type": "typo3-cms-extension",
	"description": "Fancy filelist",
	"minimum-stability": "dev",
	"license": ["GPL-2.0+"],
	"authors": [
		{
			"name": "André Bardoun",
			"email": "bardoun@schaffrath-digital.de",
			"role": "Maintainer"
		},
		{
			"name": "Jürgen Venne",
			"email": "venne@schaffrath-digital.de",
			"role": "Maintainer"
		}
	],
	"require": {
		"psr/http-message": "^1.0.0",
		"typo3/cms-backend": "9.4.*@dev",
		"typo3/cms-core": "9.4.*@dev",
		"typo3/cms-fluid": "9.4.*@dev",
		"typo3fluid/fluid": "^2.3"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.12",
		"roave/security-advisories": "dev-master",
		"typo3/testing-framework": "dev-master"
	},
	"config": {
		"bin-dir": "bin",
		"sort-packages": true
	},
	"replace": {
		"digital_asset_management": "*"
	},
	"scripts": {
		"t3g:test:php:unit": [
			"phpunit -c Build/UnitTests.xml --log-junit logs/phpunit.xml"
		],
		"t3g:test": [
			"@t3g:test:php:unit"
		],
		"t3g:cgl": [
			"php-cs-fixer fix --config Build/.php_cs.dist --format=junit > logs/php-cs-fixer.xml"
		]
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "digital_asset_management"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\DigitalAssetManagement\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\DigitalAssetManagement\\Tests\\": "Tests/"
		}
	}
}
